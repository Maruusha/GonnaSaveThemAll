{
  "name": "GonnaSaveThemAll",
  "description": "Base Level Extension",
  "version": "1.0.2",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup/store_url.html",
    "default_icon": "images/icon/icon-128.png"
  },
  "icons": {
    "16": "images/icon/icon-16.png",
    "32": "images/icon/icon-32.png",
    "48": "images/icon/icon-48.png",
    "128": "images/icon/icon-128.png"
  },
  "permissions": [
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": ["<all_url>"],
      "js": ["scripts/Content.js"]
    }
  ]
}